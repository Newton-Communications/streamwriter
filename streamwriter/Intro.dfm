  object lblManualDesc: TLabel
    Left = 16
    Top = 95
    Width = 413
    Height = 67
    Anchors = [akLeft, akRight, akBottom]
    AutoSize = False
    Caption = 
      'In order to manually record streams just doubleclick them in the' +
      ' stream-browser on the right. You can also use the stream-browse' +
      'r'#39's contextmenu. Manual recording of streams helps those people ' +
      'who use automatic recording because the titles your streamWriter' +
      ' detects are broadcasted on the streamWriter-network.'
    WordWrap = True
  end
